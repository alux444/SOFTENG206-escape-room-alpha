<Pane fx:id="room" focusTraversable="true" onKeyPressed="#onKeyPressed" onKeyReleased="#onKeyReleased" prefHeight="470.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.TableController">
   <children>
      <ImageView fx:id="backgroundImage" fitHeight="378.0" fitWidth="494.0" layoutX="54.0" layoutY="76.0">
         <image>
            <Image url="@../images/table.png" />
         </image>
      </ImageView>
      <Label layoutX="30.0" layoutY="24.0" prefHeight="41.0" prefWidth="549.0" text="SOFTENG 206 - ESCAPE ROOM">
         <font>
            <Font name="Arial" size="35.0" />
         </font>
      </Label>
      <Rectangle fx:id="rubberDuck" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff" focusTraversable="true" height="135.0" layoutX="135.0" layoutY="118.0" onMouseClicked="#clickDuck" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="135.0" />
      <Rectangle fx:id="apple" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff" focusTraversable="true" height="148.0" layoutX="125.0" layoutY="260.0" onMouseClicked="#clickApple" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="145.0" />
      <Rectangle fx:id="pencil" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff" focusTraversable="true" height="191.0" layoutX="270.0" layoutY="197.0" onMouseClicked="#clickPencil" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="80.0" />
      <Button fx:id="returnBtn" layoutX="525.0" layoutY="76.0" mnemonicParsing="false" onAction="#returnRoom" text="Return" />
      <Rectangle fx:id="scissors" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff" focusTraversable="true" height="167.0" layoutX="350.0" layoutY="250.0" onMouseClicked="#clickScissors" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="157.0" />
      <Polygon fx:id="watch" fill="DODGERBLUE" layoutX="318.0" layoutY="149.0" onMouseClicked="#clickWatch" opacity="0.0" points="-66.5, 12.0, 188.5, 113.0, 0.0, -60.0" stroke="BLACK" strokeType="INSIDE" />
   </children>
</Pane>
